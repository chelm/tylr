#!/usr/bin/env node

var nfs = require('node-fs'),
  async = require('async'),
  tylr = require('../');
 
var argv = require('optimist')
    .usage('Usage: $0 -f [string] -d [string] -t [string] -l [string] -a [center|box]')
    .demand(['f','d'])
    .default('l', '[0,5]')
    .default('a', 'center')
    .default('t', 'geojson')
    .argv;

tylr.tyle( argv );
